{
  "name": "@commonbase/sdk",
  "version": "0.0.0",
  "description": "Commonbase is a Typescript SDK for building LLM integrations and tooling",
  "repository": {
    "type": "git",
    "url": "git@github.com:commonbaseapp/sdk.git"
  },
  "keywords": [
    "commonbase",
    "sdk",
    "cms",
    "gpt-3",
    "gpt3",
    "openai"
  ],
  "author": "Commonbase Inc.",
  "license": "MIT",
  "main": "./index.js",
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "scripts": {
    "build": "tsc && cp package.json README.md ./dist/",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --ext .tsx,.ts,.json",
    "format": "prettier --write .",
    "prepublish": "pnpm build"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitest/coverage-v8": "^0.33.0",
    "axios": "0.26.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "openai": "3.2.1",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vitest": "^0.33.0"
  },
  "packageManager": "pnpm@8.1.1",
  "peerDependencies": {
    "axios": "^0.26.1",
    "openai": "^3.2.1"
  }
}
