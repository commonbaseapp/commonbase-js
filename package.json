{
  "name": "commonbase",
  "version": "0.0.1",
  "description": "Commonbase is a Typescript SDK for building LLM integrations and tooling",
  "repository": {
    "type": "git",
    "url": "git@github.com:commonbaseapp/sdk.git"
  },
  "keywords": [
    "commonbase",
    "sdk",
    "chat-gpt",
    "gpt-3",
    "gpt3",
    "openai"
  ],
  "author": "Commonbase Inc.",
  "license": "MIT",
  "main": "./index.ts",
  "publishConfig": {
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "directory": "dist"
  },
  "scripts": {
    "build": "tsc && cp package.json README.md ./dist/",
    "test": "echo 'tests passed!'",
    "release": "pnpm build && pnpm lint:fix && pnpm format && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --ext .tsx,.ts,.json",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "public": true,
  "packageManager": "pnpm@7.25.0"
}
